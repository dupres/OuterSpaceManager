<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="dupre.com.outerspacemanager.outerspacemanager">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:animateLayoutChanges="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SignUpActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
        <activity android:name=".VGActivity" />
        <activity android:name=".BuildingActivity" />
        <activity android:name=".SearchActivity" />
        <activity android:name=".FlotteActivity" />
        <activity android:name=".ChantActivity" />
        <activity android:name=".GalaxieActivity"></activity>
    </application>

</manifest>